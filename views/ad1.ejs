<!DOCTYPE html>
<html>

<head>
  <script data-ad-client="ca-pub-6351481944770696" async
    src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
  <title>administrator</title>
  <link rel="stylesheet" type="text/css" href="ad1.css">
</head>

<body onload="myFunction()">
  <div class="loader" id="campaign-alert">
    <img src="loader.png" alt="loader">
  </div>

  <script>
    function myFunction() {
      document.getElementById('campaign-alert').style.display = "none";
    }
  </script>

  <table>
    <tr>
      <th>User Id</th>
      <th>Email</th>
    </tr>

    <% usersWithSecrets.forEach(function(user){ %>

    <tr>
      <th> <%= user._id %> </th>
      <th> <%= user.username %> </th>
    </tr>

    <% }) %>

  </table>

  <hr>

  <table>
    <tr>
      <th>User Id</th>
      <th>Name</th>
      <th>Phone No</th>
      <th>Movie</th>
      <th>Seats</th>
    </tr>

    <% usersWithmovie.forEach(function(user){ %>
    <tr>
      <th><%=user.id%></th>
      <th><%=user.name%></th>
      <th><%=user.phone%></th>
      <th><%=user.moviename%></th>
      <th><%=user.seats%></th>
    </tr>
    <% }) %>

  </table>



  <hr />




</body>

</html>